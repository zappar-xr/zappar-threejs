{
  "name": "@zappar/zappar-threejs",
  "version": "1.0.0",
  "description": "ThreeJS wrappers for Zappar's computer vision library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/zappar-xr/zappar-threejs/",
  "issues": "https://github.com/zappar-xr/zappar-threejs/issues/",
  "homepage": "https://zap.works/universal-ar/",
  "scripts": {
    "build:standalone": "webpack --config=webpack.config.standalone.js --mode=production",
    "build:standalone-debug": "webpack --config=webpack.config.standalone.dev.js --mode=development",
    "build:module": "tsc",
    "lint": "eslint ./src --ext .ts ",
    "lint:fix": "eslint --fix ./src --ext .ts",
    "build:tests": "webpack --config=webpack.config.tests.js --mode=development",
    "tests": "npm run build:tests && webpack serve --mode=development --config=webpack.config.tests.js",
    "start": "npm run tests",
    "test-module": "jest puppeteer/module",
    "test-standalone": "jest puppeteer/standalone",
    "webpack-puppeteer": "webpack --config=webpack.config.puppeteer.js --mode=development && webpack serve --config=webpack.config.puppeteer.js",
    "standalone-test-serve": "concurrently 'zapworks serve --port 7010 umd' 'zapworks serve --port 7011 puppeteer-standalone-dist' || true"
  },
  "author": "Zappar Limited",
  "license": "MIT",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/**/*",
    "umd/**/*"
  ],
  "devDependencies": {
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/pixelmatch": "^5.2.2",
    "@types/pngjs": "^6.0.0",
    "@types/three": "^0.127.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@zappar/zapworks-cli": "^0.6.17",
    "concurrently": "^6.0.0",
    "eslint": "^7.17.0",
    "html-webpack-plugin": "^5.3.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer": "^7.1.0",
    "three": "^0.127.0",
    "ts-jest": "^26.5.1",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "three": "0.113.0 - 0.127.0"
  },
  "dependencies": {
    "@zappar/zappar": "0.3.8"
  }
}
